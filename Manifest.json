{
    "manifest_version": 3,
    "name": "Auto Fill Table input",
    "version": "1.0",
    "description": "Automatically fills Acceptable Quantity input with due Minimum Quantity.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }